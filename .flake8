[flake8]
max-line-length = 88
extend-ignore = E203, W503, E501
exclude = 
    .git,
    __pycache__,
    docs/source/conf.py,
    old,
    build,
    dist,
    .venv,
    venv,
    node_modules
per-file-ignores =
    __init__.py:F401
max-complexity = 10
import-order-style = google
application-import-names = app
